{
  "name": "trivivia-server",
  "version": "1.0.1",
  "description": "",
  "main": "src/server.ts",
  "scripts": {
    "test": "vitest --coverage",
    "test:prod": "tsc --project ./tsconfig.json && vitest --run",
    "start": "tsx watch --tsconfig ./tsconfig.json ./src/server.ts",
    "start:dev": "yarn start",
    "start:prod": "tsx --no-cache --tsconfig ./tsconfig.json ./src/server.ts",
    "create-initial-migration": "ts-node --project ./tsconfig.json scripts/createMigration.ts",
    "migrate": "sequelize db:migrate",
    "postinstall": "cp -n database.example.json database.json && yarn migrate"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cookie-session": "^2.0.44",
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.5",
    "sequelize-cli": "^6.6.0",
    "sequelize-mock": "^0.10.2"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cookie-session": "^2.0.0",
    "cors": "^2.8.5",
    "email-validator": "^2.0.4",
    "express": "^4.18.2",
    "html-prettify": "^1.0.7",
    "jsonwebtoken": "^9.0.0",
    "nanoid": "^3.3.6",
    "reflect-metadata": "^0.1.13",
    "sequelize": "^6.29.3",
    "sequelize-typescript": "^2.1.5",
    "shared": "1.0.0",
    "socket.io": "^4.6.1",
    "sqlite3": "^5.1.6",
    "tsx": "^4.7.0"
  }
}
